function camellia_module_theme() {
  $theme['uploader'] = [
    'template' => 'uploader',
    'variables' => ['api_key' => NULL, 'domain' => NULL,],
  ];
  $theme['image_view'] = [
    'template' => 'image_view',
    'variables' => ['image_id' => NULL,],
  ];
